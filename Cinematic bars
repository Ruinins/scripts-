   local key = Enum.KeyCode.L -- Change the keybind here if needed
local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad)

local main = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Bottom = Instance.new("Frame")

main.Name = "main"
main.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
main.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
main.ResetOnSpawn = false
main.Enabled = false

Frame.Parent = main
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.Position = UDim2.new(0, 0, 0, -218)
Frame.Size = UDim2.new(0, 4854, 0, 218)

Bottom.Parent = main
Bottom.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Bottom.Position = UDim2.new(0, 0, 1, 0)
Bottom.Size = UDim2.new(0, 4854, 0, 250)

local isUIVisible = false
local debounce = false

local function toggleUIVisibility()
    if debounce then
        return
    end
    
    debounce = true
    isUIVisible = not isUIVisible

    local tweenGoalFrame = {}
    local tweenGoalBottom = {}

    if isUIVisible then
        tweenGoalFrame.Position = UDim2.new(0, 0, -0.125, 0)
        tweenGoalBottom.Position = UDim2.new(0, 0, 1, -150)
    else
        tweenGoalFrame.Position = UDim2.new(0, 0, 0, -218)
        tweenGoalBottom.Position = UDim2.new(0, 0, 1, 0)
    end

    local tweenFrame = game:GetService("TweenService"):Create(Frame, tweenInfo, tweenGoalFrame)
    local tweenBottom = game:GetService("TweenService"):Create(Bottom, tweenInfo, tweenGoalBottom)

    tweenFrame:Play()
    tweenBottom:Play()

    if not isUIVisible then
        tweenFrame.Completed:Connect(function()
            main.Enabled = false
            debounce = false
        end)
    else
        main.Enabled = true
        debounce = false
    end

    local localPlayer = game.Players.LocalPlayer
    for _, gui in ipairs(localPlayer.PlayerGui:GetChildren()) do
        if gui:IsA("ScreenGui") and gui ~= main then
            if isUIVisible then
                gui.Enabled = false
            else
                gui.Enabled = true
            end
        end
    end
end

local function onKeyPress(input, gameProcessedEvent)
    if not gameProcessedEvent and input.KeyCode == key then
        toggleUIVisibility()
    end
end

game:GetService("UserInputService").InputBegan:Connect(onKeyPress)


